{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "noEmit": false,
    "outDir": "dist",
    "strict": false,
    "baseUrl": ".",
    "rootDir": "src",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "moduleResolution": "node",
    "declaration": true
  },
  "include": [
    "src/**/*.ts"
  ]
}